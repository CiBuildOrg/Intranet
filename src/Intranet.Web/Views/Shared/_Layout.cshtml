@inject Microsoft.ApplicationInsights.AspNetCore.JavaScriptSnippet JavaScriptSnippet
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <!--Let browser know website is optimized for mobile-->
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Certaincy Intranet</title>

    <environment names="Development">
    </environment>
    <environment names="Staging,Production">
    </environment>
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link rel="stylesheet" href="~/assets/css/materialize.light.css" asp-append-version="true" />
    @RenderSection("Styles", required: false)
    @Html.Raw(JavaScriptSnippet.FullScript)
    @Html.Partial("_GoogleAnalytics")
</head>
<body>
    <link rel="stylesheet" href="~/assets/css/materialize.complete.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/assets/css/main.css" asp-append-version="true" />
    <nav>
        @await Html.PartialAsync("_Navigation")
    </nav>
    <main>
        <div class="container">
            @RenderBody()
        </div>
    </main>
    <footer class="page-footer">
        @Html.Partial("_Footer")
    </footer>

    @Html.Partial("_Scripts")
    @RenderSection("Scripts", required: false)
</body>
</html>
