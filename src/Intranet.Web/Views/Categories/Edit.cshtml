@model Category

@{
    ViewData["Title"] = "Edit Category";
    var from = ViewData["from"] ?? String.Empty;
}

<h2>@ViewData["Title"]</h2>

<div class="row">
    <form asp-action="Edit" asp-route-from="@from" class="col s12">
        <div class="row">
            @Html.Partial("_CategoryForm")
            <div class="row">
                <input type="submit" value="Save" class="btn" />
            </div>
        </div>
    </form>
</div>

<div>
    <a asp-action="Index" asp-route-from="@from">Back to List</a>
</div>

